{
  "prod": {
    "apigateway_enabled": true,
    "apigateway_description": "Torii API. Rendering with Jinja2.",
    "app_function": "torii_api.app.app",
    "aws_environment_variables": {
      "FLASK_ENV": "production"
    },
    "aws_region": "ap-northeast-1",
    "cloudwatch_log_level": "INFO",
    "cloudwatch_data_trace": false,
    "cloudwatch_metrics_enabled": false,
    "cors": true,
    "debug": false,
    "keep_warm": true,
    "lambda_description": "Torii function - Rendering with Jinja2.",
    "memory_size": 128,
    "project_name": "torii",
    "runtime": "python3.7",
    "s3_bucket": "torii-private",
    "tags": {
      "Project": "Torii"
    },
    "timeout_seconds": 15,
    "xray_tracing": true
  }
}
